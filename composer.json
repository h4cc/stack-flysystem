{
    "name": "h4cc/stack-flysystem",
    "description": "A Stack HttpKernel for Flysystem filesystems as a single purpose application.",
    "license": "MIT",
    "keywords": [
        "stack", "http", "rest", "files"
    ],
    "authors": [
        {
            "name": "Julius Beckmann",
            "email": "github@h4cc.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "~0.5",
        "stack/builder": "~1.0",
        "stack/run": "~1.0",
        "stack/callable-http-kernel": "1.0.*@dev"
    },
    "require-dev": {
        "silpion/stack-logger": "~1.0",
        "monolog/monolog": "~1.8"
    },
    "autoload": {
        "psr-4": {"h4cc\\StackFlysystem\\": "src/"}
    },
    "extra": {
        "branch-alias": { "dev-master": "1.0-dev" }
    }
}
